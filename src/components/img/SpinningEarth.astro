---
import { Image } from "astro:assets";
import SpinningEarthIconSrc from "../../img/earth.gif";

export interface Props {
  width?: number;
  height?: number;
  id?: string;
}
const { width , height , id } = Astro.props;
---

<Image
  id={id}
  src={SpinningEarthIconSrc}
  alt="Rotating Globe"
  width={width}
  height={height}
/>
